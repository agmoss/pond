    "class-validator": "^0.12.2",
    "express-rate-limit": "^5.1.3",
    "figlet": "^1.5.0",
    "graphql": "^15.3.0",
    "graphql-tools": "^6.2.3",
    "graphql-type-json": "^0.3.2",
    "helmet": "^4.1.1",
    "nest-winston": "^1.4.0",
